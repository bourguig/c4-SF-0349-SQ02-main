{% extends 'base.html.twig' %}

{% block title %}Feedback{% endblock %}

{% block body %}
    <h1>Feedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feedback.id }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ feedback.firstName }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ feedback.lastName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ feedback.email }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ feedback.comment }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_feedback_index') }}">back to list</a>

    <a href="{{ path('app_feedback_edit', {'id': feedback.id}) }}">edit</a>

    {{ include('feedback/_delete_form.html.twig') }}
{% endblock %}
